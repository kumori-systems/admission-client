{
  "name": "admission-client",
  "version": "1.0.0",
  "description": "Client Stub for ECloud Admission",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!@foo)"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "browser": {
    "./dist/src/file-stream.d.ts": "./dist/src/file-stream-browser.d.ts",
    "./dist/src/file-stream.js": "./dist/src/file-stream-browser.js",
    "./dist/src/file-stream.js.map": "./dist/src/file-stream-browser.js.map"
  },
  "scripts": {
    "spec": "./node_modules/.bin/jest dist/test/*.test.js",
    "clean": "rm -rf dist",
    "lint": "./node_modules/.bin/tslint  --type-check --project .",
    "dist": "rm -rf dist;mkdir dist;./node_modules/.bin/tsc -p tsconfig.json --outDir dist"
  },
  "author": "jpla@iti.es",
  "license": "ISC",
  "dependencies": {
    "acs-client": "git+ssh://git@gitlab.com:ECloud/acs-client.git#master",
    "axios": "0.16.2",
    "form-data": "2.2.0",
    "socket.io-client": "2.0.3",
    "typed-event-emitter": "1.0.3"
  },
  "devDependencies": {
    "@types/form-data": "0.0.33",
    "@types/jest": "20.0.2",
    "@types/node": "7.0.22",
    "@types/socket.io-client": "1.4.29",
    "jest": "23.4.1",
    "ts-jest": "23.0.0",
    "tslint": "5.4.3",
    "tslint-config-standard": "5.0.2",
    "typescript": "2.3.3"
  }
}
